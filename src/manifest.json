{
  "name": "Quick Review for GitHub™",
  "description": "Hotkeys for faster code review of Pull Requests on GitHub™",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}
